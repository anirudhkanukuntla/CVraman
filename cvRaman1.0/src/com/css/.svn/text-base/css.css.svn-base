}
.selectedStyle {
	up-skin:Embed(source='com/Images/dot.jpg');
	down-skin:Embed(source='com/Images/dot.jpg');
	skin:Embed(source='com/Images/dot.jpg');
	over-skin:Embed(source='com/Images/dot.jpg');
} 
.unselectedStyle { 
	up-skin:Embed(source='com/Images/icon_red_dot.png');
