.navbar_myfavorites_normal{
	up-skin:Embed(source='com/Images/whiteImage.png');
	down-skin:Embed(source='com/Images/whiteImage.png');
	skin:Embed(source='com/Images/whiteImage.png');
	over-skin:Embed(source='com/Images/whiteImage.png');
	color: #298A08;
	rollOverColor: #298A08;
	fontSize:13;
	fontStyle:normal;
	fontWeight:bold;
	corner-radius:5
}
.popUpBack{
	backgroundColor:red;
	backgroundAlpha:0.6;
}
.errorTip {
	color: #FFFFFF;
