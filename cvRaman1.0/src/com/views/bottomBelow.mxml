			import mx.controls.Alert;
			import mx.events.FlexEvent;
			import mx.utils.ObjectUtil;
			private var _appControls:appControls = appControls.getInstance();
			protected function group1_creationCompleteHandler(event:FlexEvent):void
			{
				toggleId.selectedIndex = 1;
				_appControls.buttDispatcher.addEventListener(mainEvent.MAIN_EVENT_DISP, eventTrac);
				
			}
			[Bindable]
			private var _departmentName:String = "";
			public static var _profSlot:ClassFactory =  new ClassFactory(profSlot);
			private function eventTrac(e:mainEvent):void
			{
				switch (e.eventType)
				{
					case "COLLEGE_PROFILE":
						var mainCol:XML = e.obj as XML;
						_departmentName = String(mainCol.name);
						var mainlist:XMLList =  mainCol.lectures;
						var arr:ArrayCollection = new ArrayCollection;
						var index:uint=1;
						if(similarChbox1.numChildren>0){
							similarChbox1.removeAllChildren();
						}
						for each(var x:XML in mainlist){
						var ne:* = _profSlot.newInstance();
						var showsObjects:onjShows = new onjShows;
						ne.Designation = x.Designation;
						//						Alert.show( ""+x.Name);
						ne.Name = x.name;
						ne.Qualification = x.Qualification;
						ne.Exp = x.Exp;
						ne.showbool=false;
						similarChbox1.addChild(ne);
						var lect:lecNames = new lecNames;
						lect.Designation = x.Designation;
						lect.Name = x.name;
						lect.SNo = index;
						lect.Qualification = x.Qualification;
						lect.Exp = x.Exp;
						arr.addItem(lect);
						index++;
					}
						toggleId.selectedIndex = 2;
						_appControls.depArray= arr;
						_appControls.depArray.refresh();
						break;
					case "DEPARTMENT":
						var mainCol:XML = e.obj as XML;
						_departmentName = String(mainCol.name);
						var mainlist:XMLList =  mainCol.lectures;
						var arr:ArrayCollection = new ArrayCollection;
						var index:uint=1;
						if(similarChbox.numChildren>0){
							similarChbox.removeAllChildren();
						}
						for each(var x:XML in mainlist){
						var ne:* = _profSlot.newInstance();
						var showsObjects:onjShows = new onjShows;
						ne.Designation = x.Designation;
//						Alert.show( ""+x.Name);
						ne.Name = x.name;
						ne.Qualification = x.Qualification;
						ne.Exp = x.Exp;
						similarChbox.addChild(ne);
						var lect:lecNames = new lecNames;
						lect.Designation = x.Designation;
						lect.Name = x.name;
						lect.SNo = index;
						lect.Qualification = x.Qualification;
						lect.Exp = x.Exp;
						arr.addItem(lect);
						index++;
					}
						_appControls.depArray= arr;
						_appControls.depArray.refresh();
						break;
					case "BOTTOM_VISIBLE":
//						if(){
//						toggleId.selectedIndex = 1;
//						}
						break;
					case "HOME_VIEW":
						toggleId.selectedIndex = 1;
						aboutcvraman.source="com/Images/kakatiya-gate-1.jpg";
						break;
					case "ABOUT_VIEW":
						toggleId.selectedIndex = 2;
						aboutcvraman.source="com/Images/cvramanImage.png";
						break;
					case "INFRASTRUCTURE_VIEW":
						toggleId.selectedIndex = 4;
						break;
					case"CONTACT_VIEW":
//						Alert.show("CONTACT_VIEW")
						toggleId.selectedIndex = 3;
						break;
					case "VIEW_CHANGE":
						var dateNow:Date = new Date();
						var str:String = "";
						str =  "?uniqueID="+dateNow.minutes+dateNow.seconds+dateNow.milliseconds;
						var str1:String = e.obj.viewID;
						switch(str1){
							case"Mathematics":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departMath+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 0;
								break;
							case"Botany":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departBot+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 1;
								break;
							case"Zoology":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departZoo+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 2;
								break;
							case"Physics":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departPhy+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 3;
								break;
							case"Chemistry":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departChe+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 4;
								break;
							case"Computer Science":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departCS+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 5;
								break;
							case"Commerce":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departCom+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 6;
								break;
							case"English":
								_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._departEng+str,"DEPARTMENT");
								toggleId.selectedIndex = 0;
								//								toggleId.selectedIndex = 7;
								break;
						}
						break;
				}
			}
			private var borderWeight:uint=5;
			private function onMapReady(event:Event):void {
				mapView.setCenter(new LatLng(18.879444,79.438889), 18, MapType.NORMAL_MAP_TYPE);
				var cosVBbox:VBox = new VBox;
				cosVBbox.setStyle("backgroundColor","0x01325a");
				cosVBbox.setStyle("backgroundAlpha","0.8");
				cosVBbox.setStyle("cornerRadius","5");
				var mailabl1:mx.controls.Label = new mx.controls.Label;
				var mailabl2:mx.controls.Label = new mx.controls.Label;
				var mailabl3:mx.controls.Label = new mx.controls.Label;
				var mailabl4:mx.controls.Label = new mx.controls.Label;
				var mailabl5:mx.controls.Label = new mx.controls.Label;
				//one
				mailabl1.setStyle("fontWeight","bold");
				mailabl1.setStyle("fontStyle","italic");
				mailabl1.setStyle("color","#FFFFFF");
				mailabl1.setStyle("fontSize","20");
				//two
				mailabl2.setStyle("fontWeight","bold");
				mailabl2.setStyle("fontStyle","italic");
				mailabl2.setStyle("color","#FFFFFF");
				mailabl2.setStyle("fontSize","20");
				//three
				mailabl3.setStyle("fontWeight","bold");
				mailabl3.setStyle("fontStyle","italic");
				mailabl3.setStyle("color","#FFFFFF");
				mailabl3.setStyle("fontSize","20");
				//four
				mailabl4.setStyle("fontWeight","bold");
				mailabl4.setStyle("fontStyle","italic");
				mailabl4.setStyle("color","#FFFFFF");
				mailabl4.setStyle("fontSize","20");
				//five
				mailabl5.setStyle("fontWeight","bold");
				mailabl5.setStyle("fontStyle","italic");
				mailabl5.setStyle("color","#FFFFFF");
				mailabl5.setStyle("fontSize","20");
				
				mailabl1.text = "CVRaman Degree college";
				mailabl2.text = "Hanuman nagar";
				mailabl3.text = "Mancheriyal";
				mailabl4.text = "504208";
				mailabl5.text = "Adilabad";
				cosVBbox.addChild(mailabl1);
				cosVBbox.addChild(mailabl2);
				cosVBbox.addChild(mailabl3);
				cosVBbox.addChild(mailabl4);
				cosVBbox.addChild(mailabl5);
				mapView.openInfoWindow(mapView.getCenter(), new InfoWindowOptions({customContent:cosVBbox,hasShadow:true,hasCloseButton:false,ALIGN_CENTER:1,hasTail:true}));
			}
			//com/Images/cvramanImage.png
			
			protected function vbox1_clickHandler(event:MouseEvent):void
			{
				mapHolder.visible=true;
				callusHolder.visible = false;
				emailHolder.visible = false;
				SocialHolder.visible = false;
			}
			
			protected function callusHolder_clickHandler(event:MouseEvent):void
			{
				callusHolder.visible = true;
				mapHolder.visible=false;
				emailHolder.visible = false;
				SocialHolder.visible = false;
			}
			
			protected function vbox2_clickHandler(event:MouseEvent):void
			{
				callusHolder.visible = false;
				mapHolder.visible=false;
				emailHolder.visible = true;
				SocialHolder.visible = false;
			}
			
			protected function vbox3_clickHandler(event:MouseEvent):void
			{
				callusHolder.visible = false;
				mapHolder.visible=false;
				emailHolder.visible = false;
				SocialHolder.visible = true;
			}
			
			protected function image1_clickHandler(event:MouseEvent):void
			{
				// TODO Auto-generated method stub
				mailme(event.currentTarget.name);
			}
			
			private function mailme(str:String):void
			{
				var u:URLRequest = new URLRequest("mailto:" + str);
				navigateToURL(u,"_self");
			}
			

			protected function label1_clickHandler(event:MouseEvent):void
			{
				if(event.currentTarget.id==missionHolder.id){
					missionHolder.setStyle("borderColor","0xC8FE2E");
					viewsfordetails.selectedIndex=0;
					similarChbox1.visible=false;
					depnameID.visible = false;
				}else{
					missionHolder.setStyle("borderColor","#084B8A");
				}
					
				if(event.currentTarget.id==profHolder.id){
					profHolder.setStyle("borderColor","0xC8FE2E");
					_appControls._xmlLoader.XML_LOAD(_appControls.navigationPath._colProf,"COLLEGE_PROFILE");
					viewsfordetails.selectedIndex=1;
					similarChbox1.visible=true;
					depnameID.visible = true;
				}else{
					profHolder.setStyle("borderColor","#084B8A");
				}
				
				if(event.currentTarget.id==uniHolder.id){
					uniHolder.setStyle("borderColor","0xC8FE2E");
					viewsfordetails.selectedIndex=2;
					similarChbox1.visible=false;
					depnameID.visible = false;
				}else{
					uniHolder.setStyle("borderColor","#084B8A");
				}
				
				if(event.currentTarget.id==organiHolder.id){
					organiHolder.setStyle("borderColor","0xC8FE2E");
				}else{
					organiHolder.setStyle("borderColor","#084B8A");
				}
				
			}
			
			protected function navigateFun(event:MouseEvent):void
			{
				
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
